<?xml version="1.0" encoding="utf-8" ?>
<widget id="TabWidget.widget.TabWidget" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>TabWidget</name>
    <description>This widget will change the tab caption of the default tab container. The tab caption can be populated with any string attribute.</description>

    <icon></icon>
    

    <properties>
        <property key="tabname" type="string" required="true" defaultValue="">
            <caption>TabName</caption>
            <category>Appearance</category>
            <description>Name of the Tab Name property</description>
        </property>

        <property key="newcaption" type="attribute">
            <caption>New Name Attribute</caption>
            <category>Appearance</category>
            <description>New caption to show</description>
            <attributeTypes>
                <attributeType name="String" />
            </attributeTypes>
        </property>

    </properties>
</widget>
